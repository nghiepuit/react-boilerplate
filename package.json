{
  "name": "demo-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:sw/wap": "NODE_OPTIONS=--max-old-space-size=10240 react-sdk --platform=wap --target=service-worker --mode=production",
    "build:node/wap": "NODE_OPTIONS=--max-old-space-size=10240 react-sdk --platform=wap --target=node --mode=production",
    "build:browser/wap": "NODE_OPTIONS=--max-old-space-size=10240 react-sdk --platform=wap --target=web --mode=production",
    "build:sw/web": "NODE_OPTIONS=--max-old-space-size=10240 react-sdk --platform=web --target=service-worker --mode=production",
    "build:node/web": "NODE_OPTIONS=--max-old-space-size=10240 react-sdk --platform=web --target=node --mode=production",
    "build:browser/web": "NODE_OPTIONS=--max-old-space-size=10240 react-sdk --platform=web --target=web --mode=production",
    "build:web": "yarn build:sw/web && yarn build:browser/web && yarn build:node/web",
    "build:wap": "yarn build:sw/wap && yarn build:browser/wap && yarn build:node/wap",
    "dev": "yarn dev:web",
    "dev:web": "NODE_OPTIONS=--max-old-space-size=10240 react-sdk --platform=web --target=web --mode=development",
    "dev:wap": "NODE_OPTIONS=--max-old-space-size=10240 react-sdk --platform=wap --target=web --mode=development",
    "start": "node --no-deprecation build/backend/server.js",
    "start:debug": "cross-env NODE_ENV=development node --no-deprecation --inspect --require dotenv/config build/backend/server.js",
    "start:local": "cross-env NODE_ENV=production node --no-deprecation --require dotenv/config build/backend/server.js",
    "test": "NODE_OPTIONS=--max-old-space-size=10240 react-sdk --mode=test --target=web --setupTest='./setupTest.js'",
    "test:web": "yarn test --platform=web",
    "test:web/coverage": "yarn test --platform=web --jest-coverage",
    "test:wap": "yarn test --platform=wap",
    "test:wap/coverage": "yarn test --platform=wap --jest-coverage",
    "test:all": "yarn test --platform=all",
    "test:all/coverage": "yarn test --platform=all --jest-coverage"
  },
